os\r\n          .get('/api/v1/projects')\r\n          .then(response => {\r\n            this.projects = response.data\r\n            console.log(this.projects)\r\n          })\r\n          .catch(error=>{\r\n            throw error\r\n          })\r\n\r\n        this.loading(false)\r\n        \r\n      }\r\n    }\r\n      \r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  h1{\r\n    font-family: monospace;\r\n    color: rgb(14, 111, 175);\r\n  }\r\n  .container{\r\n      display: inline-block;\r\n      width: auto;\r\n      margin: 5px;\r\n      margin-bottom: 5vh !important;\r\n      \r\n    }\r\n    .page{\r\n      width: 100%;\r\n      font-family: monospace;\r\n    }\r\n    h2{\r\n      margin: 5px;\r\n    }\r\n    .typed-out{\r\n      \r\n      overflow: hidden;\r\n      font-size: 40px;\r\n      border-right: .15em solid rgb(61, 158, 223);\r\n      white-space: nowrap;\r\n      animation: \r\n      typing 1.5s steps(20, end) forwards;\r\n      width: 0;\r\n    }\r\n    @keyframes typing {\r\n      from { width: 0 }\r\n      to { width: 100% }\r\n    }\r\n    a{\r\n        text-decoration: none;\r\n        color: rgb(61, 158, 223) !important;\r\n        \r\n    }\r\n</style>"],"mappings":"AAcE,SAASA,aAAY,QAAS,UAAU;AACxC,SAASC,UAAS,QAAS,OAAO;AAClC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,wBAAuB;AAE7C,eAAc;EACZC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAC;IACTF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,QAAQ,EAAC;EAChB,CAAC;EACDC,OAAO,EAAC;IACN,GAAGT,UAAU,CAACD,aAAa,EAAE,CAAC,SAAS,CAAC,CAAC;IACzCS,QAAQA,CAAA,EAAE;MAER,IAAI,CAACE,OAAO,CAAC,IAAI;MAEjBT,KAAI,CACDU,GAAG,CAAC,kBAAkB,EACtBC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACP,QAAO,GAAIO,QAAQ,CAACR,IAAG;QAC5BS,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,QAAQ;MAC3B,CAAC,EACAU,KAAK,CAACC,KAAK,IAAE;QACZ,MAAMA,KAAI;MACZ,CAAC;MAEH,IAAI,CAACP,OAAO,CAAC,KAAK;IAEpB;EACF;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}