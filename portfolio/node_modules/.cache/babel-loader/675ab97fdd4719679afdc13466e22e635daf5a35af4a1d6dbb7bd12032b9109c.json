      },\r\n        mounted(){\r\n            slides = document.getElementsByClassName('carousel-box');\r\n            \r\n            len =  slides.length;\r\n            this.length = len;\r\n            this.slides = slides;\r\n            \r\n        },\r\n        methods: {\r\n            changeslide(n){\r\n                lastIndex = this.index\r\n                if(this.index === this.length-1 && n === 1){\r\n                    this.index = 0;\r\n                }else if(this.index === 0 && n === -1){\r\n                    this.index = this.length--;\r\n                } else{\r\n                    this.index += n;\r\n                }\r\n                showSlide(this.index ,lastIndex);\r\n            },\r\n            showSlide(current ,lastIndex){\r\n                slides = document.getElementsByClassName('carousel-box');\r\n                slides[lastIndex].className.replace(\" active\", \"\");\r\n                slides[current].className += \"active\"\r\n            }\r\n        }\r\n\r\n    }\r\n    \r\n</script>\r\n\r\n<style scoped>\r\n    #carousel{\r\n        max-width: 700px;\r\n        max-height: 500px;\r\n        position: relative;\r\n        margin: auto;\r\n    }\r\n    .carousel-box{\r\n        display:none;\r\n    }\r\n    .carousel-img{\r\n        width: 100%;\r\n        height: 80%;\r\n    }\r\n    .active{\r\n        display: block !important;\r\n    }\r\n    .previous, .next{\r\n        position: absolute;\r\n        top: 40%;\r\n    }\r\n    .previous{\r\n        left: 8px;\r\n    }\r\n    .next{\r\n        right: 8px;\r\n    }\r\n</style>\r\n"],"mappings":"AAcI,eAAe;EACXA,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACHC,MAAM,EAAEC;EACZ,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAE,CAAC;MACRC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACLC,MAAK,GAAIC,QAAQ,CAACC,sBAAsB,CAAC,cAAc,CAAC;IAExDC,GAAE,GAAKH,MAAM,CAACF,MAAM;IACpB,IAAI,CAACA,MAAK,GAAIK,GAAG;IACjB,IAAI,CAACH,MAAK,GAAIA,MAAM;EAExB,CAAC;EACDI,OAAO,EAAE;IACLC,WAAWA,CAACC,CAAC,